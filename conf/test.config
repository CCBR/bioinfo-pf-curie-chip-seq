/*
 * -------------------------------------------------
 *  Nextflow config file for running tests
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a fast and simple test. Use as follows:
 *   nextflow run nf-core/chipseq -profile test
 */

params {
  // Limit resources so that this can run on Travis
  max_cpus = 4
  max_memory = 24.GB
  max_time = 4.h
  // Input data
  // TODO: Specify the paths to your test data
  // TODO: Give any required params for the test so that command line flags are not needed
  singleEnd = false
  samplePlan = "$baseDir/test-op/sampleplan.csv"
  preseq_defect = true
}
